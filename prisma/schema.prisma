generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserToken {
  id           Int      @id @default(autoincrement())
  sessionId    String   @unique
  accessToken  String
  refreshToken String
  expiresAt    DateTime
}

model Parlor {
  id          Int      @id @default(autoincrement())
  name        String
  website     String
  members     String[]
  note        String
}